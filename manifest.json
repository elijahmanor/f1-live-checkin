{
  "name": "Fellowship Bible Church: Command Central",
  "description": "This extension adds additional features to the command central page to assist with Command Central during the check-in process at Fellowship Bible Church.",
  "version": "0.1",
  "background": {
    "scripts": [ "background.js" ]
  },
  "icons": { "128": "fbc_128x128.jpg" },
  "options_page": "options.html",
  "permissions": [
    "tabs", "https://portal.fellowshipone.com/*"
  ],
  "browser_action": {
    "default_title": "Options",
    "default_icon": "fbc_16x16.jpg"
  },
  "content_scripts": [
    {
      "matches": [ "https://portal.fellowshipone.com/*" ],
      "css": [ "bootstrap.css", "bootstrap-responsive.css", "toastr.css", "toastr-responsive.css", "jquery-ui.css" ],
      "js": [ "jquery-1.8.0.min.js", "bootstrap.js", "toastr.js", "jquery.sparkline.js", "jquery-ui.js", "gauge.min.js", "raphael.2.1.0.min.js", "justgage.1.0.1.min.js", "fellowshipone.js" ]
    }
  ],
  "web_accessible_resources": [
    "tos-intercom.ogg", "https://www.google.com/jsapi"
  ],
  "manifest_version": 2
}


